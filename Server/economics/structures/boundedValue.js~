//  Data structure to hold intermediate results for our Econ evaluation
var boundedValue = function()
{	
	var start;
	var mid;
	var end;			
	var beforeAverageTQFirm;
	var afterAverageTQFirm;	
	var beforeAverageProfFirm;
	var afterAverageProfFirm;
	var beforeAverageTQSIC;
	var afterAverageTQSIC;
	var beforeAverageProfSIC;
	var afterAverageProfSIC;			
	var quarterlyIntervalTQDifference; 
	var quarterlyIntervalProfDifference;		
	var sic;	
	var cusip;	
	var state;	
	var quarterSpan;
		
	return {
		start: 0,
		mid: 0,
		end: 0,
		state: "",
		beforeAverageTQFirm: 0,
		afterAverageTQFirm: 0,
		beforeAverageProfFirm: 0,
		afterAverageProfFirm: 0,
		beforeAverageTQSIC: 0,
		afterAverageTQSIC: 0,
		beforeAverageProfSIC: 0,
		afterAverageProfSIC: 0,
		quarterlyIntervalTQDifference: 0,
		quarterlyIntervalProfDifference: 0,
		sic: "",
		cusip: "",
		quarterSpan: 0
	};	
}
